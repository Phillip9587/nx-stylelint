{
  "name": "nx-stylelint-src",
  "version": "17.0.0",
  "description": "Stylelint Plugin for Nx",
  "author": "Phillip Barta",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^18.13.0 || >=20.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "nx": "nx",
    "build": "nx build",
    "test": "nx test",
    "prepare": "is-ci || husky install",
    "release": "standard-version --no-verify",
    "release:beta": "standard-version --no-verify --prerelease beta"
  },
  "devDependencies": {
    "@commitlint/cli": "18.0.0",
    "@commitlint/config-conventional": "18.0.0",
    "@nx/devkit": "17.0.2",
    "@nx/eslint": "17.0.2",
    "@nx/eslint-plugin": "17.0.2",
    "@nx/jest": "17.0.2",
    "@nx/js": "17.0.2",
    "@nx/plugin": "17.0.2",
    "@nx/workspace": "17.0.2",
    "@swc-node/register": "1.6.8",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.95",
    "@types/jest": "29.5.6",
    "@types/node": "18.18.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jsonc-eslint-parser": "2.4.0",
    "nx": "17.0.1",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "standard-version": "9.5.0",
    "stylelint": "15.11.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "verdaccio": "5.27.0"
  }
}
