{
  "name": "nx-stylelint-src",
  "version": "15.0.0",
  "description": "Stylelint Plugin for Nx",
  "author": "Phillip Barta",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >= 18.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "nx": "nx",
    "build": "nx build",
    "test": "nx test",
    "prepare": "is-ci || husky install",
    "release": "standard-version --no-verify",
    "release:beta": "standard-version --no-verify --prerelease beta"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@nx/devkit": "16.10.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/plugin": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@swc-node/register": "1.6.8",
    "@swc/core": "1.3.92",
    "@types/jest": "29.5.1",
    "@types/node": "14.18.43",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "jsonc-eslint-parser": "2.3.0",
    "nx": "16.10.0",
    "nx-cloud": "16.4.0",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "standard-version": "9.5.0",
    "stylelint": "15.6.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.1.6"
  }
}
